	<resources base="http://www.school-30.com/api/v0.1/">
		<resource id="stat" path="stat">
			<doc title="Статистика архива">
			</doc>
			<method name="GET" id="FindStatisticsUseCase">
				<doc title="Запрос на получение статистики">
				</doc>
				<request>
					<param name="Accept" style="header">
						<doc>application/xml</doc>
					</param>
					<param name="Accept" style="header">
						<doc>application/json</doc>
					</param>
				</request>
				<response status="200">
					<param name="Etag" style="header" />
					<representation mediaType="application/xml; charset=UTF-8" element="ns2:Statistics">
						<doc title="OK" >Accept header application/xml</doc>
					</representation>
					<representation mediaType="application/json; charset=UTF-8">
						<doc title="OK">Accept header application/json</doc>
					</representation>
				</response>
				<response status="304">
					<param name="Etag" style="header" />
				</response>
				<response status="550">
					<representation mediaType="text/plain">
						<doc title="Internal server error">
							Если возникла ошибка при работе с базой или другие серверные ошибки, то ответ содержит текст ошибки "Internal server error".
						</doc>
					</representation>
				</response>
			</method>
		</resource>
